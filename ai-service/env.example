# Server Configuration
HOST=0.0.0.0
PORT=8000
DEBUG=false
ENVIRONMENT=development
WORKER_PROCESSES=1

# Security
API_KEY=your-secure-api-key-here
ALLOWED_ORIGINS=*
ALLOWED_HOSTS=*

# TensorFlow AI Model Configuration
MODEL_PATH=/app/models/mobilenet_v2
MODEL_TYPE=mobilenet_v2
MODEL_CACHE_SIZE=1

# Processing Configuration
MAX_FILE_SIZE_MB=10
BATCH_SIZE_LIMIT=10
DEFAULT_CONFIDENCE_THRESHOLD=0.5

# Database Configuration (Supabase)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-supabase-anon-key
SUPABASE_SERVICE_KEY=your-supabase-service-key
SUPABASE_STORAGE_BUCKET=part-images

# Google Search API (Part search fallback)
GOOGLE_API_KEY=your-google-api-key
GOOGLE_SEARCH_ENGINE_ID=your-search-engine-id

# External Part Database APIs
OCTOPART_API_KEY=your-octopart-nexar-api-key

# Redis (Optional)
REDIS_URL=redis://localhost:6379

# Monitoring
SENTRY_DSN=https://your-sentry-dsn
LOG_LEVEL=INFO
METRICS_ENABLED=true

# Performance
MAX_CONCURRENT_REQUESTS=100
REQUEST_TIMEOUT=30 